        <div class="col-sm-8">
          <div class="jumbotron">
            <h1>Add Books</h1>
          </div>
          <!-- <form method="GET" action="/about"> -->
          <form class="form-row" onsubmit="return handleSubmit()">
            <% keys.forEach(i => { %>
              <div class="form-group col-md-6">
                <label for="exampleInputEmail1"><%= i %></label>
                <input type="text" class="form-control" id="<%= i %>" aria-describedby="emailHelp">
              </div>
            <% }); %>
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>

        </div>
      </div>
    </main>

    <footer><%- include('../partials/footer'); %></footer>
