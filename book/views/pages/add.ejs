<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body class="container">
    <header><%- include('../partials/header'); %></header>

    <script>
        const handleSubmit = e => {
            // debugger;
            e.preventDefault();
            
        }
    </script>

    <main class="d-flex align-items-center justify-content-center">
